{
  "name": "react-functional-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:worker": "cd workers-api && npm run build",
    "dev:worker": "cd workers-api && npm run dev",
    "deploy:worker": "cd workers-api && npm run deploy",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "build:all": "npm run build && npm run build:worker",
    "deploy:all": "npm run deploy && npm run deploy:worker",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:open",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:run",
    "generate": "plop",
    "deploy": "wrangler pages publish dist --project-name=reacttemplate"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.363.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@cloudflare/wrangler": "^3.0.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/cypress": "^0.1.6",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cypress": "^14.2.1",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^4.2.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "plop": "^4.0.1",
    "postcss": "^8.5.3",
    "react-router-dom": "^7.4.0",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
